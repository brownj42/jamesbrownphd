@Article{Hecker2003,
author ="Hecker, A. and Scheele, I. and Havenith, M.",
title  ="{IR}-High resolution spectroscopy of the {N}$_2${O} dimer: The torsional mode",
journal  ="Physical Chemistry Chemical Physics",
year  ="2003",
volume  ="5",
issue  ="11",
pages  ="2333-2336",
publisher  ="The Royal Society of Chemistry",
doi  ="10.1039/B300733M",
url  ="http://dx.doi.org/10.1039/B300733M",
abstract  ="The rotational resolved IR-spectrum of the NO-NO van der Waals complex has been observed in the region of 2244-2252 cm using a computer controlled lead salt diode laser spectrometer. This paper reports on the first observation of a van der Waals mode in (NO). 1569 absorption lines could be assigned to the combination of the fundamental -asymmetric stretch of NO-monomer with the -van der Waals mode (torsion) of the NO-dimer. The band origin of the spectrum is observed at = 2247.83 cm which is 20.0 cm above the excitation of the NO dimer. This deviates considerably from the theoretically predicted value at 37.84 cm (C. Dutton{,} )"}