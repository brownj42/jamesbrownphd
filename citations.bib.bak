@Article{Vissers2003,
author ="Vissers, G. W. M. and Wormer, P. E. S. and van der Avoird, A.",
title  ="An ab initio $\mbox{CO}$ dimer interaction potential and the computation of the rovibrational spectrum of $(\mbox{CO})_2$",
journal  ="Phys. Chem. Chem. Phys.",
year  ="2003",
volume  ="5",
issue  ="21",
pages  ="4767-4771",
publisher  ="The Royal Society of Chemistry",
doi  ="10.1039/B309468E",
url  ="http://dx.doi.org/10.1039/B309468E",
abstract  ="A full potential energy surface (PES) is computed for a dimer consisting of rigid CO molecules. The coupled cluster single double plus perturbative triples [CCSD(T)] method is used with the augmented correlation consistent polarized valence triple zeta atomic orbital basis (aug-cc-pVTZ) with bond orbitals in the middle of the CO-CO bond. The PES is fitted in terms of analytic functions and the fitted potential is used to compute the lowest rovibrational states of the dimer. Semi-quantitative agreement with recent millimeter-wave experiments (M. D. Brookes and A. R. W. McKellar{,} {,} 1999{,} {,} 7321-7328; D. A. Roth{,} L. A. Surin{,} B. S. Dumesh{,} G. Winnewisser and I. Pak{,} {,} 2000{,} {,} 3034-3038 and J. Tang {,} {,} 2002{,} {,} 87-93) is found."
}

