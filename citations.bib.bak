@article{Frazao1996,
	author={J. M. A. Frazao and J. M. C. Lourenco and Cunha Aurea  M. and M. F. Laranjeira and J. Los and A. M. C. Moutinho},
	year={1996},
	title={The sulfur reaction in small ionized carbonyl sulfide clusters},
	journal={Journal of Chemical Physics},
	volume={104},
	number={21},
	pages={8393-8404},
	abstract={Ionic species obtained by electron impact on (OCS)n clusters produced by expansion of OCS mixed with argon at two different (OCS/argon) concns. are studied as a function of stagnation pressure and electron energy. Ionization efficiency curves of homogeneous cluster ions (OCS)n+, homogeneous sulfur cluster ions Sm+ and inhomogeneous ions (OCS)nSm+ are analyzed. Several reactions paths, ending in Sm+, have been identified. The strong correlation between reaction path and cluster size is discussed. Finally, by minimization of the potential function created by dipole-dipole, dipole-quadrupole, and quadrupole-quadrupole interactions between OCS mols., we propose the geometrical configurations of neutral (OCS)n (n=2-5) clusters. [on SciFinder(R)]},
	keywords={sulfur reaction ionized carbonyl sulfide cluster; geometry carbonyl sulfide cluster},
	isbn={0021-9606}
}

