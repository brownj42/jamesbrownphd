@Article{Dehghany2009,
author ="Dehghany, M. and Afshari, Mahin and Abusara, Z. and Moazzen-Ahmadi, N.",
title  ="Nonpolar nitrous oxide dimer: fundamentals of the mixed $^{14}\mbox{N}_2\mbox{O}-^{15}\mbox{N}_2\mbox{O}$ dimer and new combination bands of ($^{14}\mbox{N}_2\mbox{O}$)$_2$ and ($^{15}\mbox{N}_2\mbox{O}$)$_2$ involving the $B_u$ intermolecular bend",
journal  ="Phys. Chem. Chem. Phys.",
year  ="2009",
volume  ="11",
issue  ="35",
pages  ="7585-7588",
publisher  ="The Royal Society of Chemistry",
doi  ="10.1039/B905373E",
url  ="http://dx.doi.org/10.1039/B905373E",
abstract  ="Spectra of the nonpolar nitrous oxide dimer in the region of the N2O [small nu]1 fundamental band are observed in a pulsed supersonic slit jet expansion probed with a tunable diode laser. Four bands are analysed: two fundamentals of the mixed 14N2O-15N2O dimer and combination bands involving the intermolecular disrotation of the monomers (Bu intermolecular bend) for both (14N2O)2 and (15N2O)2. Because the determination of this intermolecular frequency relies on the experimentally unknown frequency of the (forbidden) symmetric fundamental{,} we used previously published ab initio results and their proximity to our experimental values to assign the upper state of the combination bands. The resulting intermolecular disrotation frequencies are 42.3(1.0) and 41.6(1.0) cm-1 for the (14N2O)2 and (15N2O)2{,} respectively. This represents the first observation of the mixed 14N2O-15N2O dimer{,} and the direct determination of a second intermolecular frequency for the nonpolar (N2O)2."}

